                            <ui:Label tabindex="-1" text="Running:" display-tooltip-when-elided="true" name="running_label" style="border-left-width: 4px;" />
                            <ui:Label tabindex="-1" text="20 ms" display-tooltip-when-elided="true" name="running_time" style="-unity-font-style: bold;" />
                        </ui:VisualElement>
                        <ui:VisualElement name="label_1_area" style="flex-grow: initial; background-color: rgba(0, 0, 0, 0); flex-direction: row; flex-shrink: 1; min-height: 21px; height: 20px;">
                            <ui:VisualElement name="helping_color" style="flex-grow: 1; background-color: rgb(107, 107, 107); flex-shrink: 1; max-height: 20px; min-height: 20px; max-width: 20px; min-width: 20px; border-right-width: 2px;" />
                            <ui:Label tabindex="-1" text="Helping:" display-tooltip-when-elided="true" name="helping_label" style="border-left-width: 4px;" />
