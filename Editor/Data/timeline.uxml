                            <ui:VisualElement name="horizontal_align" style="flex-grow: 1; flex-direction: row; height: 16px; max-height: 16px; min-height: 16px;">
                                <ui:VisualElement name="depends_on_color" style="flex-grow: 0; width: 14px; height: 14px; background-color: rgb(255, 255, 0); flex-shrink: 0;" />
                                <ui:Label text="What depends on this job(s)" name="depends_on_text" style="padding-left: 4px; -unity-font-style: bold;" />
                            </ui:VisualElement>
                        </ui:VisualElement>
                        <ui:VisualElement name="dependant_on_list_area" style="flex-grow: 1; align-self: stretch; align-items: stretch; justify-content: flex-start; flex-shrink: 0; min-height: 100px; flex-direction: column; flex-wrap: nowrap; flex-basis: initial; padding-bottom: 4px;">
                            <ui:VisualElement name="horizontal_align" style="flex-grow: 1; flex-direction: row; height: 16px; max-height: 16px; min-height: 16px;">
                                <ui:VisualElement name="depends_on_color" style="flex-grow: 0; width: 14px; height: 14px; background-color: rgb(255, 255, 0); flex-shrink: 0;" />
